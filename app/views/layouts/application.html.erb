<!DOCTYPE html>
<html>
<head>
  <title><%= full_title(yield(:title)) %></title>
  <%= stylesheet_link_tag "application", media: "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <%= render 'layouts/shim' %>
  
  <link rel="icon" type="image/png" href="https://s3.amazonaws.com/concerts-assets/images/favicon.ico">
</head>
<body>
<div class="secondary-header">
		<%= render 'layouts/headbanner' %>
</div>
<div class="container" id="data-pjax-container" data-pjax-container>
  <%= yield %>
  <% flash.each do |key, value| %>
      <%= content_tag(:div, value, class: "alert alert-#{key}") %>
  <% end %>
  <%= debug(params) if Rails.env.development? %>
</div>
  <%= render 'layouts/deleter' %>
  <%= render 'layouts/maxprice'%>
</body>
</html>