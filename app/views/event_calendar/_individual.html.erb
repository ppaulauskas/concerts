<div class="event-individual" id="<%= Random.new.rand(10...100) %>">
	<div class="star">
		<% if Random.new.rand(1...10) == 1 %>
			<img src="https://s3.amazonaws.com/concerts-assets/images/blk_star_full.png"></div>
		<% else %>
			<img src="https://s3.amazonaws.com/concerts-assets/images/blk_star_empty.png"></div>
		<% end %>
	<div class="curr-event"><h4><%= (Event.find_by eventtime: @d.yesterday).band %></h4></div>
	<div class="cross"><img src="https://s3.amazonaws.com/concerts-assets/images/blk_cross_empty.png"></div>
</div>